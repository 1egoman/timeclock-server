<% include header %>

  <div class="error container">
    <h1>A slight problem...</h1>
    <% if (msg === "Not found") { %>
      <h3>404</h3>
      <p>
        We cannot find this repo. If you think there should be a repo here, try
        logging in <a href="/auth/github">here</a> first.
      </p>
    <% } else if (ENV_DEVELOPMENT) { %>
      <pre><%= msg %></pre>
    <% } else { %>
      <pre><%= msg %></pre>
      <p>Sometimes, all you need to do it log in <a href="/auth/github">here</a> first.</p>
    <% } %>
  </div>
  
<% include footer %>
